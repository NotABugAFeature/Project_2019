CC=gcc -std=gnu11

FLAGSTEST= -DSORTED_PROJECTIONS -DsERIAL_PROJECTIONS -DsERIAL_EXECUTION -DsERIAL_JOIN -DsERIAL_SELFJOIN -DsERIAL_FILTER -DsERIAL_SORTING -DsERIAL_PRESORTING -DMAX_QUERIES_LIMIT -DoNE_QUERY_AT_A_TIME

all: queries_fast

queries_fast: execute_query.c middle_list.c main.c query.c queue.c quicksort.c radix_sort.c relation.c sort_merge_join.c string_list.c table.c job_fifo.c job_scheduler.c projection_list.c list_array.c
	$(CC) $(FLAGSTEST) -O3 -g -Wall -o queries execute_query.c middle_list.c main.c query.c queue.c quicksort.c radix_sort.c relation.c sort_merge_join.c string_list.c table.c job_fifo.c job_scheduler.c projection_list.c list_array.c -lpthread
	
clean:
	rm -f *.o queries
